// Căutarea liniară.

#include <stdio.h>
#include <stdlib.h>
int main()
{
int v[100],i,n,x;
printf("\nDati numarul de elemente: ");
scanf("%d", &n);
for(i=0;i<n;i++)
{
printf("\nElementul %d =",i);
scanf("%d",&v[i]);
}
print(" \nDati elementul pe care doriti sa il cautati:");
scanf("%d",&x)
for(i=0;i<n;i++)
if(v[i]==x)
printf("\nElementul cautat ( %d ) a fost gasit pe pozitia %d",x,i+1);
system("pause");
return 0;
}
